{
    "name": "donzo24/orange-sms",
    "type": "library",
    "license": "MIT",
    "keywords": ["orange", "orange-api", "sms"],
    "description": "A php library to interact with the orange sms api for MiddleEast and Africa.",
    "authors": [
        {
            "name": "Youssouf DONZO",
            "email": "donzoyoussouf@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2||^8.0",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.11",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Donzo24\\Orange\\SMS\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orange\\SMS\\Tests\\": "tests/"
        }
    }
}
